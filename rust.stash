impl <v>Point<> {
	fn new(<v>x<>: i32, <v>y<>: i32) -> Self {
		<v>Point<> {<v>x<>, <v>y<>}
	}
}

let <v>point_1<> = <v>Point<>::new(10, 20);

println!("Point 1: {}, {}", <v>point_1<>.<v>x<>, <v>point_1<>.<v>y<>);

<#>// Prints: "Point 1: 10, 20"<><>
